    "import time\n",
    "import threading\n",
    "from threading import Thread\n",
    "\n",
    "def sleep(i):\n",
    "    print('sleep ',i)\n",
    "    time.sleep(2)\n",
    "    print('awake ',i)\n",
    "\n",
    "for i in range(10):\n",
    "    th = Thread(target=sleep, args=(i,))\n",
    "    th.start()\n",
    "    print(threading.active_count())"
   ]
